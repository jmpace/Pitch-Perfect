    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "next": "^15.3.3",
    "openai": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^3.3.1",
    "task-master-ai": "^0.16.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^11.3.0",
